#include <stdio.h>
#include <string.h>

int main()
{
    char str[100];
    int i,len,j,count=0;
    len=sizeof(str);
    printf("enter the string:");
    scanf("%s",str);
    for(i=0;str[i]!='\0';i++){
        if(str[i] >= '0' && str[i] <='9'){
            count = count + str[i]-'0';
        }
        
    }
    printf("the length of the string is:%d",count);
    return 0;
}
