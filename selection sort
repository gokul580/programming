// Online C compiler to run C program online
#include <stdio.h>

int main()
{
    int i,j,a[5],t;
    printf("enter the array elements:");
    for(i=0;i<5;i++)
    scanf("%d",&a[i]);
    for(i=0;i<4;i++)
    {
        for(j=i+1;j<5;j++)
        {
            if(a[i]>a[j])
            {
            t=a[i];
            a[i]=a[j];
            a[j]=t;
        }}
       
    }
     for(i=0;i<5;i++)
     printf(" %d",a[i]);
     
    return 0;
}
