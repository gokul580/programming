#include <stdio.h>

int main() {
    int arr[5],i,big,small;
    printf("enter the array:");
    for(i=0;i<5;i++){
        scanf("%d",&arr[i]);
    }
    big=arr[0];
    small=arr[0];
    for(i=1;i<5;i++){
        if(arr[i]>big ){
            big=arr[i];
        }
    
    }
    for(i=1;i<5;i++){
        if(arr[i]<small){
            small=arr[i];
        }
    
    }
    printf("the biggest element is %d\n",big);
    printf("the smallest element is %d",small);
    return 0;
}
