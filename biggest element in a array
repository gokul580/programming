#include <stdio.h>

int main()
{
    int i,a[5],n,big;
    n=sizeof(a[0])/sizeof(a);
    printf("enter the array elements:");
    for(i=0;i<5;i++)
    scanf("%d",&a[i]);
    big = a[0];
    for(i=1;i<5;i++){
        if(a[i]>big)
        big=a[i];
    }
    printf("the big element in a array is %d:",big);
    return 0;
}
