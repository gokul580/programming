#include <stdio.h>

int main()
{
    int i,a[5],n,small;
    n=sizeof(a[0])/sizeof(a);
    printf("enter the array elements:");
    for(i=0;i<5;i++)
    scanf("%d",&a[i]);
    small = a[0];
    for(i=1;i<5;i++){
        if(a[i]<small)
        small=a[i];
    }
    printf("the small element in a array is %d:",small);
    return 0;
}
