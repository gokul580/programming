#include <stdio.h>

int fibo(int n)
{
    if(n==1)
    return 1;
    else if(n==0)
    return 0;
    else 
    return fibo(n-1)+fibo(n-2);
}
void main(){
    
    int fib=0,i,t,ret;
    printf("enter the terms:");
    scanf(" %d",&t);
    for(i=1;i<=t;i++){
        
    ret = fibo(fib);
    printf(" %d",ret);
    fib ++;
    }
}
